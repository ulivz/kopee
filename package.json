{
  "name": "kopy",
  "version": "0.0.1",
  "description": "A high-level interface for succinct files coping",
  "main": "lib/index.js",
  "files": [
    "lib",
    "!lib/tsconfig.tsbuildinfo",
    "!lib/*.map"
  ],
  "scripts": {
    "bootstrap": "pnpm i --prefer-offline",
    "test": "jest",
    "cov": "jest --coverage",
    "lint": "eslint src/** __tests__ --ext .js,.jsx,.ts,.tsx --fix",
    "ci": "npm run lint && npm run cov",
    "build": "rm -rf lib && tsc",
    "dev": "npm run code:build -- --watch",
    "release": "publish",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "url": "git@github.com:ulivz/copy.git",
    "type": "git"
  },
  "author": "chenhaoli <chenhaoli@nomadlandance.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "debug": "^4.1.1",
    "fs-extra": "10.0.0",
    "globby": "^9.0.0",
    "majo": "^0.9.1",
    "tildify": "^1.2.0",
    "tslib": "^2.0.0",
    "micromatch": "4.0.4"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@speedy-js/eslint-config": "^0.1.3",
    "@speedy-js/publish": "^0.1.3",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "^25.1.3",
    "@vuepress/plugin-medium-zoom": "^1.3.1",
    "eslint": "^7.4.0",
    "eslint-config-sherry": "0.0.1",
    "jest-cli": "^24.8.0",
    "ts-jest": "^25.2.1",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
